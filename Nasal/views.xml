<?xml version="1.0" encoding="ISO-8859-1"?>

<PropertyList>

  <chase-distance-m type="double" archive="y"> -15 </chase-distance-m>

  <view n="101">
    <name>Tail Camera View</name>
    <type>lookfrom</type>
    <internal archive="y">true</internal>
    <config>
      <from-model type="bool">true</from-model>
      <from-model-idx type="int">0</from-model-idx>
      <ground-level-nearplane-m type="double">0.01f</ground-level-nearplane-m>
      <x-offset-m archive="y"> 0.000 </x-offset-m> <!--Right-->
      <y-offset-m archive="y"> 1.897 </y-offset-m> <!--Up-->
      <z-offset-m archive="y"> 4.600 </z-offset-m> <!--Back-->
      <pitch-offset-deg> -5 </pitch-offset-deg>
      <default-field-of-view-deg type="double"> 97 </default-field-of-view-deg>
    </config>
  </view>

  <view n="102">
    <name>surfer</name>
    <internal archive="y">true</internal>
    <type>lookfrom</type>
    <config>
      <internal type="bool">false</internal>
      <from-model type="bool">false</from-model>
      <from-model-idx type="int">0</from-model-idx>
      <eye-lat-deg-path>/position/latitude-deg</eye-lat-deg-path>
      <eye-lon-deg-path>/position/longitude-deg</eye-lon-deg-path>
      <eye-alt-ft-path>/position/altitude-ft</eye-alt-ft-path>
      <eye-heading-deg-path>/orientation/heading-deg</eye-heading-deg-path>
      <eye-pitch-deg-path>/orientation/pitch-deg</eye-pitch-deg-path>
      <eye-roll-deg-path>/orientation/roll-deg</eye-roll-deg-path>
      <at-model type="bool">false</at-model>
      <at-model-idx type="int">0</at-model-idx>
      <ground-level-nearplane-m type="double">0.1f</ground-level-nearplane-m>
      <heading-offset-deg>0</heading-offset-deg>
      <x-offset-m archive="y"> -105 </x-offset-m> <!-- Right -->
      <y-offset-m archive="y">   50 </y-offset-m> <!-- Up    -->
      <z-offset-m archive="y"> -180 </z-offset-m> <!-- Back  -->
      <pitch-offset-deg> 0 </pitch-offset-deg>
      <default-field-of-view-deg> 120 </default-field-of-view-deg>
    </config>
  </view>

  <view n="103">
    <name>surf view</name><type>lookfrom</type>
    <config>
      <from-model type="bool">true</from-model>
      <from-model-idx type="int">0</from-model-idx>
      <x-offset-m archive="y"> -1.65 </x-offset-m> <!-- Right -->
      <y-offset-m archive="y"> -1.05 </y-offset-m> <!-- Up    -->
      <z-offset-m archive="y"> -4.24 </z-offset-m> <!-- Back  -->
      <pitch-offset-deg> 0 </pitch-offset-deg>
      <default-field-of-view-deg> 110 </default-field-of-view-deg>
    </config>
  </view>
  
  <view n="104">
    <name>Cabin</name>
    <internal archive="y">true</internal>
    <type>lookfrom</type>
    <config>
      <dynamic-view type="bool">true</dynamic-view>
      <from-model type="bool">true</from-model>
      <ground-level-nearplane-m type="double">0.5f</ground-level-nearplane-m>
      <x-offset-m archive="y"> 0.15 </x-offset-m> <!-- Right -->
      <y-offset-m archive="y"> 0.44 </y-offset-m> <!-- Up    -->
      <z-offset-m archive="y"> 0.16 </z-offset-m> <!-- Back  -->
      <pitch-offset-deg> 0 </pitch-offset-deg>
      <default-field-of-view-deg> 90 </default-field-of-view-deg>
    </config>
  </view>

  <view n="105">
    <name>Copilot</name>
    <internal archive="y">true</internal>
    <type>lookfrom</type>
    <config>
      <dynamic-view type="bool">true</dynamic-view>
      <from-model type="bool">true</from-model>
      <ground-level-nearplane-m type="double">0.5f</ground-level-nearplane-m>
      <x-offset-m archive="y">  0.31161 </x-offset-m> <!-- Right -->
      <y-offset-m archive="y">  0.94526 </y-offset-m> <!-- Up    -->
      <z-offset-m archive="y"> -2.48124 </z-offset-m> <!-- Back  -->
      <pitch-offset-deg> 0 </pitch-offset-deg>
      <default-field-of-view-deg> 97 </default-field-of-view-deg>
    </config>
  </view>

  <view n="0">
    <internal archive="y">true</internal>
    <config>
      <x-offset-m archive="y"> -0.31161 </x-offset-m> <!-- Right -->
      <y-offset-m archive="y">  1.03000 </y-offset-m> <!-- Up    -->
      <z-offset-m archive="y"> -2.48124 </z-offset-m> <!-- Back  -->
      <pitch-offset-deg> 0 </pitch-offset-deg>
      <default-field-of-view-deg> 97 </default-field-of-view-deg>
    </config>
  </view>

  <view n="1">
    <config>
      <target-z-offset-m archive="y" type="double"> 0 </target-z-offset-m>
    </config>
  </view>

  <view n="2">
    <config>
      <target-z-offset-m archive="y" type="double"> 0 </target-z-offset-m>
    </config>
  </view>

  <view n="3">
    <config>
      <target-z-offset-m archive="y" type="double"> 0 </target-z-offset-m>
    </config>
  </view>

  <view n="4">
    <config>
      <target-z-offset-m archive="y" type="double"> 0 </target-z-offset-m>
    </config>
  </view>

  <view n="5">
    <config>
      <target-z-offset-m archive="y" type="double"> 0 </target-z-offset-m>
    </config>
  </view>

  <view n="6">
    <config>
      <target-z-offset-m archive="y" type="double"> 0 </target-z-offset-m>
    </config>
  </view>

</PropertyList>
