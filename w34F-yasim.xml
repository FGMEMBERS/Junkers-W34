<?xml version="1.0" encoding="ISO-8859-1"?>

<!-- Junkers W34 Floats   : BARANGER Emmanuel with Melchior script (2013)

     Sources              : http://en.wikipedia.org/wiki/Junkers_W_34

     Wingspan             : 17.75 m  ( 58 ft 2 3/4 in )
     Length               : 10.27 m  ( 33 ft 8 1/4 in )
     Height               :  3.53 m  ( 11 ft 7 in     )
     Empty weight         :  1700 kg (    3748 lb)
     Engine               : 1 x BMW 132 radial engine (660 hp)
     Maximum speed        : 265 km/h ( 143 kt )
     Cruise speed         : 233 km/h ( 126 kt )
     Service ceiling      :   6300 m ( 20670 ft )
-->

<airplane mass="3748" >

  <approach speed="40" aoa="4" fuel="0.2">
    <control-setting axis="/controls/engines/engine[0]/throttle" value="0.2"/>
    <control-setting axis="/controls/engines/engine[0]/mixture" value="0.65"/>
    <control-setting axis="/controls/gear/gear-down" value="1"/>
  </approach>

  <cruise speed="126" alt="20670" fuel="0.5" >
    <control-setting axis="/controls/engines/engine[0]/throttle" value="1.0" />
    <control-setting axis="/controls/engines/engine[0]/mixture" value="0.65" />
    <control-setting axis="/controls/gear/gear-down" value="0"/>
  </cruise>

  <cockpit x="1.665" y="0.300" z="0.713"/>

  <fuselage ax=" 4.465" ay="0" az=" 0.274" bx=" 2.854" by="0" bz=" 0.200" width="1.143" taper="0.70" midpoint="1"/>
  <fuselage ax=" 2.854" ay="0" az=" 0.200" bx="-0.274" by="0" bz=" 0.200" width="1.143" taper="1.00" midpoint="1"/>
  <fuselage ax="-0.274" ay="0" az=" 0.200" bx="-4.105" by="0" bz="-0.005" width="1.143" taper="0.40" midpoint="0"/>

  <!-- Wing up -->
  <wing x="1.908" y="0" z="-0.432"
        chord="2.936"
        length="8.870"
        dihedral="4"
        sweep="2"
        taper="0.5"
        camber="0.01">
        <stall aoa="18" width="1" peak="1.5"/>
        <flap0 start="0.44" end="1" lift="1.2" drag="1.1"/>

        <control-input axis="/controls/flight/aileron" control="FLAP0" split="true"/>
        <control-input axis="/controls/flight/aileron-trim" control="FLAP0" split="true"/>

        <control-output control="FLAP0" side="left" prop="/surface-positions/left-aileron-pos-norm"/>
        <control-output control="FLAP0" side="right" prop="/surface-positions/right-aileron-pos-norm"/>
  </wing>

  <hstab x="-4.25152" y="0" z="0.40578"
	 chord="1.525"
	 length="2.685"
         taper="1"
         sweep="0">
         <stall aoa="16" width="1" peak="1.5"/>
         <flap0 start="0" end="1" lift="1.3" drag="1.3"/>

         <control-input axis="/controls/flight/elevator" control="FLAP0"/>
         <control-input axis="/controls/flight/elevator-trim" control="FLAP0"/>

         <control-output control="FLAP0" prop="/surface-positions/elevator-pos-norm"/>
  </hstab>

  <vstab x="-4.22266" y="0" z="-0.18406"
	 chord="1.979"
	 length="1.795"
         taper="0.60"
         sweep="10">
         <stall aoa="14" width="1" peak="1.5"/>
         <flap0 start="0" end="1" lift="1.6" drag="1.2"/>
         <control-input axis="/controls/flight/rudder" square="true" control="FLAP0" invert="true"/>
         <control-input axis="/controls/flight/rudder-trim" control="FLAP0" invert="true"/>
         <control-output control="FLAP0" prop="/surface-positions/rudder-pos-norm" min="1" max="-1"/>
  </vstab>

  <!-- Engine    source            : http://en.wikipedia.org/wiki/BMW_132

                 name              : BMW 132
                 type              : Nine-cylinder single-row supercharged air-cooled radial engine
                 power cruise      : 789 hp for takeoff
                                   : 779 hp at 2290 rpm at 2900 m (9515 ft)
                 Displacement      : 27.7 l ( 1690 in 3)
                 weight            : 525 kg ( 1157 lb)
  -->
  <propeller x="4.60823" y="0" z="0.274"
             radius="1.66"
             mass="1157"
             moment="10"
             cruise-power="779"
             cruise-speed="126"
             cruise-rpm="2290"
             cruise-alt="9515"
             takeoff-power="789"
             takeoff-rpm="3000">
             <actionpt x="5.07367" y="0" z="0.274" />
             <piston-engine eng-power="789"
                            eng-rpm="3000"
                            alt="20670">
                            <control-input control="THROTTLE" axis="/controls/engines/engine[0]/throttle"/>
                            <control-input control="STARTER" axis="/controls/engines/engine[0]/starter"/>
                            <control-input control="MAGNETOS" axis="/controls/engines/engine[0]/magnetos"/>
                            <control-input control="MIXTURE" axis="/controls/engines/engine[0]/mixture"/>
             </piston-engine>
  </propeller>

  <!-- Flotteurs -->
  <!-- Avant 4/5 -->
  <gear x="5.48998" y="1.66597" z="-2.94733"
        compression="1.5" 
        spring = "15" 
        sfric = "0.85" 
        dfric = "0.75" 
        ignored-by-solver="0"
        on-water="1" 
        on-solid="1" 
        reduce-friction-by-extension="1.15"
        speed-planing="25" 
        spring-factor-not-planing="0.25">
  </gear>

  <gear x="5.48998" y="-1.66597" z="-2.94733"
        compression="1.5" 
        spring = "15" 
        sfric = "0.85"
        dfric = "0.75" 
        ignored-by-solver="0"
        on-water="1" 
        on-solid="1" 
        reduce-friction-by-extension="1.15"
        speed-planing="25" 
        spring-factor-not-planing="0.25">
  </gear>

  <!-- Central 6/7 -->
  <gear x="2.56" y="1.66597" z="-3.08489" 
        compression="1.2" 
        spring = "15" 
        sfric = "0.8" 
        dfric = "0.7" 
        ignored-by-solver="0"
        on-water="1" 
        on-solid="1" 
        reduce-friction-by-extension="1.15"
        speed-planing="15" 
        spring-factor-not-planing="0.4">
  </gear>

  <gear x="2.56" y="-1.66597" z="-3.08489"
        compression="1.2" 
        spring = "15" 
        sfric = "0.8" 
        dfric = "0.7" 
        ignored-by-solver="0"
        on-water="1" 
        on-solid="1" 
        reduce-friction-by-extension="1.15"
        speed-planing="15" 
        spring-factor-not-planing="0.4">
  </gear>
 
  <!-- Arriere 8/9 -->
  <gear x="-1.98959" y="1.66597" z="-2.85789" 
        compression="1.8" 
        spring = "8" 
        sfric = "0.9" 
        dfric = "0.8" 
        ignored-by-solver="1"
        on-water="1" 
        on-solid="1" 
        reduce-friction-by-extension="0.9"
        speed-planing="25" 
        spring-factor-not-planing="0.85">
  </gear>

  <gear x="-1.98959" y="-1.66597" z="-2.85789"
        compression="1.8" 
        spring = "8" 
        sfric = "0.9" 
        dfric = "0.8" 
        ignored-by-solver="1"
        on-water="1" 
        on-solid="1" 
        reduce-friction-by-extension="0.9"
        speed-planing="25" 
        spring-factor-not-planing="0.85">
  </gear>

  <!-- Fuel: 250 lbs -->
  <tank x="2.50" y=" 1.27" z="-0.42" capacity="500"/>
  <tank x="1.56" y=" 0.00" z="-0.40" capacity="250"/>
  <tank x="2.50" y="-1.27" z="-0.42" capacity="500"/>
  
  <!-- Mass displacment -->
  <ballast x="4" y="0" z="-2" mass="400" />

</airplane>
